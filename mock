public static ConnectApi.BatchResult makeTestaddToCart() {
        
        ConnectApi.BatchResult pr = new ConnectApi.BatchResult();
        List<ConnectApi.CartItemInput> results = new List<ConnectApi.CartItemInput>();

        for (Product2 prod : [SELECT Id
        FROM Product2 WHERE StockKeepingUnit IN :skusToPullForTest]) {
            ConnectApi.CartItemInput cartItem = new ConnectApi.CartItemInput();
            cartItem.productId = prod.Id;
            cartItem.quantity ='1';
            results.add(cartItem);
        }
        pr.cartItems = results;
        System.debug('pr: ' + pr);
        return pr;
    }
